<div>
    <ul class="bxslider">
        @foreach (var image in Model)
        {
            <li><img src="@Url.Content(image.ImagePath)"/></li>
        }
    </ul>
</div>
<script>
    $(document).ready(function () {
        $('.slider').bxSlider();
        $('.bx-wrapper').width('70%');
    });
</script>
